<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Estimated_Settlement_to_Creditors__c</fullName>
    <formula>Estimated_Total_Debt__c *
(
    VALUE(
        SUBSTITUTE(
            BLANKVALUE(TEXT(Est_Settlement__c), &quot;0%&quot;),
            &quot;%&quot;,
            &quot;&quot;
        )
    ) / 100
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Estimated Settlement to Creditors</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
